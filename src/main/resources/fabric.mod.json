{
  "schemaVersion": 1,
  "id": "heavierhorizons",
  "version": "${version}",
  "name": "HeavierHorizons",
  "description": "A Minecraft mod focused on steel, steel armor, and special tools.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/heavierhorizons/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.example.heavierhorizons.client.HeavierHorizonsDataGenerator"
    ],
    "client": [
      "com.example.heavierhorizons.client.HeavierHorizonsClient"
    ],
    "main": [
      "com.example.heavierhorizons.HeavierHorizons"
    ]
  },
  "mixins": [
    "heavierhorizons.mixins.json",
    {
      "config": "heavierhorizons.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
